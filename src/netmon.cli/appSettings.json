{
  "Logging": {
    "LogLevel": {
      "Default": "Trace",
      "netmon": "Trace",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    },
    "Console": {
      "LogLevel": {
        "Default": "Trace",
        "netmon": "Trace",
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information"
      },
      "FormatterName": "json",
      "FormatterOptions": {
        "SingleLine": true,
        "IncludeScopes": true,
        "TimestampFormat": "HH:mm:ss ",
        "UseUtcTimestamp": true,
        "JsonWriterOptions": {
          "Indented": true
        }
      }
    }
  },
  "AllowedHosts": "*",
  "netmon": {
    "cli": {
      "mode": "TraceRouteThenPing",
      "addresses": "8.8.8.8,151.101.0.81",
      "": ""
    }
  }
}